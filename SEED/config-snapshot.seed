### INITIALIZATION ###
CONFIG_TARGET_rockchip=y
CONFIG_TARGET_rockchip_armv8=y
CONFIG_TARGET_MULTI_PROFILE=y
CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r4s-enterprise=y
CONFIG_TARGET_DEVICE_PACKAGES_rockchip_armv8_DEVICE_friendlyarm_nanopi-r4s-enterprise=""
CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopc-t6=y
CONFIG_TARGET_DEVICE_PACKAGES_rockchip_armv8_DEVICE_friendlyarm_nanopc-t6="luci-app-frps luci-i18n-frps-zh-cn"
CONFIG_TARGET_PER_DEVICE_ROOTFS=y
CONFIG_TARGET_KERNEL_PARTSIZE=32
CONFIG_TARGET_ROOTFS_PARTSIZE=960
# CONFIG_TARGET_ROOTFS_SQUASHFS is not set

### AGGRESSIVE ###
CONFIG_BPF_TOOLCHAIN_BUILD_LLVM=y
CONFIG_KERNEL_DEBUG_INFO_BTF=y
# CONFIG_KERNEL_DEBUG_INFO_REDUCED is not set
CONFIG_DEVEL=y
CONFIG_TOOLCHAINOPTS=y
CONFIG_GCC_USE_VERSION_11=y
#CONFIG_NODEJS_16=y
CONFIG_NODEJS_ICU_NONE=y
CONFIG_TESTING_KERNEL=y
CONFIG_LINUX_6_6=y

### BASIC ###
CONFIG_PACKAGE_autocore=y
CONFIG_PACKAGE_btop=y
CONFIG_PACKAGE_coremark=y
CONFIG_PACKAGE_default-settings-chn=y
CONFIG_PACKAGE_dos2unix=y
CONFIG_PACKAGE_ethtool=y
CONFIG_PACKAGE_kmod-nft-fullcone=y
CONFIG_PACKAGE_kmod-nft-tproxy=y

### BUSYBOX ###
CONFIG_BUSYBOX_CUSTOM=y
CONFIG_BUSYBOX_CONFIG_FEATURE_SYSLOG_INFO=y

### LUCI ###
